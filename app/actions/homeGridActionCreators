var Marty = require('marty');
var GridConstants = require('../constants/gridConstants');

var HomeGridActionCreators = Marty.createActionCreators({
  getPage: GridConstants.GET_PAGE(function (filter, sortColumn, sortAscending, page, pageSize) {
    this.dispatch(filter, sortColumn, sortAscending, page, pageSize);
  })
});

module.exports = HomeGridActionCreators;
